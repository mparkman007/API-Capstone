<%- include("partials/header.ejs") %>
<body id="profile">
<% let info = JSON.parse(content)
   let data = info[0] %>



<div class="container-fluid">
    <script>
        setTimeout(function(){
            history.pushState(null, null, "<%= newUrl %>");
        });
        // Check if the page has been reloaded before using sessionStorage
    if (!sessionStorage.getItem('reloaded')) {
        // Set the flag in sessionStorage to indicate that a reload is triggered
        sessionStorage.setItem('reloaded', 'true');
        window.location.reload();
    }
    </script>

    <section id="advanced-chart">

<!-- TradingView Widget BEGIN -->
<div class="tradingview-widget-container" style="height:100%;width:100%">
    <div class="tradingview-widget-container__widget" style="height:calc(100% - 32px);width:100%"></div>
    <div class="tradingview-widget-copyright"><a href="https://www.tradingview.com/" rel="noopener nofollow" target="_blank"><span class="blue-text">Track all markets on TradingView</span></a></div>
    <script type="text/javascript" src="https://s3.tradingview.com/external-embedding/embed-widget-advanced-chart.js" async>
    {
    "autosize": true,
    "symbol": "NYSE:"+data.symbol,
    "interval": "D",
    "timezone": "Etc/UTC",
    "theme": "dark",
    "style": "1",
    "locale": "en",
    "allow_symbol_change": true,
    "calendar": false,
    "support_host": "https://www.tradingview.com"
    };
    </script>
  </div>
  <!-- TradingView Widget END -->

    </section>


 
  




    <div class="row">
    <div id="summary" class="row col-md-12 bg-body-tertiary">
        <h2>Company Profile: <%= data.symbol %></h2>
        <div class="table-responsive small">
            <table class="table table-stried table-sm">
                <thread><tr>
                    <th scope="col"></th>
                    <th scope="col"></th>
                    <th scope="col"></th>
                </tr></thread>
                <tbody>
                    <tr>
                        <td>Company Name: <%=data.companyName%></td>
                        <td>Price: <%=data.price%></td>
                        <td>Volume Average: <%=data.volAvg%></td>
                    </tr>
                    <tr>
                        <td>Market Cap: <%=data.mktCap%></td>
                        <td>Last Div: <%=data.lastDiv%></td>
                        <td>Range: <%=data.range%></td>
                    </tr>
                    <tr>
                        <td>Changes: <%=data.changes%></td>
                        <td>Currency: <%=data.currency%></td>
                        <td>CIK: <%=data.cik%></td>
                    </tr>
                    <tr>
                        <td>Exchange: <%=data.exchangeShortName%></td>
                        <td>Industry: <%=data.industry%></td>
                        <td>CEO: <%= data.ceo %></td>
                    </tr>
                    <tr>
                        <td>Sector: <%=data.sector%></td>
                        <td>Country: <%=data.country%></td>
                        <td>Fulltime Employees: <%= data.fullTimeEmployees %></td>
                    </tr>
                    <tr>
                        <td>State: <%=data.state%></td>
                    </tr>
                </tbody>


            </table>



        </div>


    </div>
    <div id="description" class="col-md-8">
        <h3>About:</h3>
        <%= data.description%>
    </div>
    </div>
  </div>


  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>




